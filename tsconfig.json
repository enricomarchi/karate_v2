{
	"extends": "./.nuxt/tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"skipLibCheck": true,
		"noImplicitAny": false,
		"types": ["node", "nuxt", "@nuxt/types"],
		"paths": {
			"~/*": ["./*"],
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.vue", "server/api/export.mjs"]
}
